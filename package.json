{
  "name": "@kumbaya_xyz/default-token-list",
  "version": "1.0.1",
  "description": "The Kumbaya default token list",
  "type": "module",
  "main": "build/index.js",
  "exports": {
    ".": {
      "import": "./build/index.js",
      "require": "./build/kumbaya-default.tokenlist.json"
    },
    "./tokenlist.json": "./build/kumbaya-default.tokenlist.json"
  },
  "scripts": {
    "test": "mocha 'test/**/*.cjs'",
    "build": "rimraf build && mkdir -p build && node src/write.cjs > build/kumbaya-default.tokenlist.json && echo 'import tokenList from \"./kumbaya-default.tokenlist.json\" with { type: \"json\" };\\nexport default tokenList;' > build/index.js",
    "prepublishOnly": "npm test && npm run build"
  },
  "files": [
    "build/kumbaya-default.tokenlist.json",
    "build/index.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kumbaya_xyz/default-token-list.git"
  },
  "keywords": [
    "uniswap",
    "default",
    "token",
    "list"
  ],
  "author": "Moody Salem & Joeh",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/kumbaya_xyz/default-token-list/issues"
  },
  "homepage": "https://github.com/kumbaya_xyz/default-token-list#readme",
  "devDependencies": {
    "@ethersproject/address": "^5.0.2",
    "@uniswap/token-lists": "^1.0.0-beta.29",
    "ajv": "^6.12.3",
    "bs58": "^6.0.0",
    "chai": "^4.2.0",
    "mocha": "^8.0.1",
    "rimraf": "^3.0.2"
  }
}
